{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "target": "es6",
    "sourceMap": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "module": "commonjs",
    "rootDir": "./",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules"
  ],
}